<%= link_to "Sign Out", destroy_user_session_path(), method: :delete   %> 

<p id="notice"><%= notice %></p>

<%= link_to 'Back to input', input_path %>
<div class="grid-cols-1">
<% @data_groups.each do |data_group| %>
  <div class='flex flex-col justify-center data-group green-accent border-current border-2 bg-white rounded p4' id='data-group-<%= data_group.id %>'>
    <div>
    <%= line_chart data_group.measurement_data.pluck(:graph_time, :value), class: "w-full" %>
    </div>
    <div>
    <%= link_to data_group.name, data_group_path(data_group), class: "text-md text-primary text-center w-1/3"%>
    </div>
  </div>
<% end %>

<%= link_to 'New Data Group', new_data_group_path %>
